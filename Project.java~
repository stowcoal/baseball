import java.io.*;

public class Project{
    public static void main(String[] args){
	if ( args.length > 0 )
	    String gameId = args[0];
	else
	    String gameId = new String("2013_10_03_pitmlb_slnmlb_1");
	WebParser wp = new WebParser(gameId);

	away = wp.ParseRoster(gameId, "away");
	home = wp.ParseRoster(gameId, "home");
	
	plays = wp.ParseAtBats(gameId);
    }
}
